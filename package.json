{
  "name": "chainide-admin",
  "version": "6.0.0-beta.1",
  "private": true,
  "description": "admin dashboard for ChainIDE",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "dev": "cross-env max dev",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && npm run lint:css && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:css": "stylelint ./src/**/*.less --fix",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:prettier": "prettier -c --write \"**/*.{js,jsx,ts,tsx,json,md}\" --end-of-line auto",
    "openapi": "max openapi",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "umi-serve",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{less,css,sass,scss}": "npm run lint:css",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/plots": "^1.2.1",
    "@ant-design/pro-components": "^1.1.1",
    "@umijs/route-utils": "^2.1.3",
    "antd": "^4.20.0",
    "classnames": "^2.3.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.0",
    "moment": "^2.29.0",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/max": "^4.0.0-rc.22",
    "@umijs/openapi": "^1.3.0",
    "@white-matrix/eslint-config": "1.1.1",
    "@white-matrix/prettier-config": "1.1.1",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.0",
    "stylelint": "14.2.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-prettier": "2.0.0",
    "swagger-ui-dist": "^4.12.0",
    "typescript": "^4.5.0",
    "umi-presets-pro": "^1.0.1",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
